<?xml version="1.0" encoding="utf-8"?>
<mmgr_config>
	<modem_link>
		<flash>
			<link_hsic device="![CDATA[/dev/ttyACM0]]" pid="0x0716" vid="0x8087"/>
		</flash>
		<baseband>
			<link_hsic device="![CDATA[/dev/ttyACM0]]" pid="0x0452" vid="0x1519"/>
		</baseband>
		<power
			device="![CDATA[/sys/devices/pci0000:00/0000:00:10.0/L2_autosuspend_enable]]"
			on="1"
			off="0"
		/>
		<link_ctrl
			device="![CDATA[/sys/devices/pci0000:00/0000:00:10.0/hsic_enable]]"
			on="1"
			off="0"
			reset="1"
		/>
	</modem_link>
	<mdm_ctrl
		type="ioctl"
		ipc_ready_present="true"
	/>
	<flashless>
		<fw_backup
			nvm_calib="/factory/telephony/calib.nvm"
		/>
		<fw_runtime
			folder="/config/telephony"
			mdm_fw="/config/telephony/modembinary.fls"
			mdm_inj_fw="/config/telephony/injected.modembinary.fls"
			nvm_static="/config/telephony/static.nvm"
			nvm_dyn="/config/telephony/dynamic.nvm"
			nvm_calib="/config/telephony/calib.nvm"
			nvm_tlv="/config/telephony/patch_nvm.tlv"
			rnd="/config/telephony/rnd_cert.bin"
		/>
	</flashless>
	<modem_comm>
		<mux
			frame_size="1509"
			retry="24"
		/>
	</modem_comm>
	<modem_timings
		ipc_ready="5"
		cd_ipc_ready="10"
	/>
	<modem_recovery
		enable="true"
		cold_reset="5"
		platform_reboot="1"
		reset="600"
		delay_before_reboot="1"
		cold_reset_timeout="10"
		shutdown_timeout="1"
	/>
	<clients_config max="12"/>
	<mcdr>
		<general
			enable="true"
			protocol="ymodem"
			folder="/logs/modemcrash"
			timeout="180"
		/>
