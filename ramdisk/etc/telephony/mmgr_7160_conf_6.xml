			<link_hsic device="![CDATA[/dev/ttyACM0]]" pid="0xF000" vid="0x1519"/>
		</link>
		<power
			device="![CDATA[/sys/devices/pci0000:00/0000:00:10.0/L2_autosuspend_enable]]"
			on="1"
			off="0"
		/>
		<link_ctrl
			device="![CDATA[/sys/devices/pci0000:00/0000:00:14.0/hsic_enable]]"
			on="1"
			off="0"
			reset="1"
		/>
	</mcdr>
</mmgr_config>
